  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:test": "cross-env NODE_ENV=test PORT=5174 tsx server/index.ts",
    "kill-server": "kill-port 5000",
    "build": "cross-env VITE_GOOGLE_CLIENT_ID=$VITE_GOOGLE_CLIENT_ID vite build && esbuild server/production.ts --platform=node --bundle --format=esm --outfile=dist/index.js --packages=external",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "tsx scripts/runMigrations.ts",
    "db:migrate:datavault": "tsx scripts/applyDatavaultMigrations.ts",
    "db:seed": "tsx scripts/seed.ts",
    "set-admin": "tsx scripts/setAdmin.ts",
    "generate-fake-data": "tsx scripts/generateFakeData.ts",
    "test-gemini": "tsx scripts/testGeminiApi.ts",
    "test-gemini-direct": "tsx scripts/testGeminiApiDirect.ts",
    "slack:test": "node scripts/slackNotifier.js",
    "test": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "cross-env VITEST_INTEGRATION=true vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "ci:test": "npm run test:integration",
    "metrics:rollup": "tsx scripts/runMetricsRollup.ts",
    "metrics:sli": "tsx scripts/computeSLIs.ts"
  },
